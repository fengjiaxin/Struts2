<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
    
<struts>
    <constant name="struts.ui.theme" value="becky" />
	<constant name="struts.ui.templateDir" value="template" />
	
	<package name="addEnv" extends="struts-default">
		<action name="addEnv" class="com.beckysolomon.action.MainAction">
			<result name="success">/resultsPage.jsp</result>
			<result name="login">/entryPage.jsp</result>
			<result name="input">/entryPage.jsp</result> <!-- Default Struts2 redirect when entryPage input invalid -->
		</action>
	</package>
</struts>